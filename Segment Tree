#include <iostream>
#include <stdio.h>

using namespace std;
int n, A[100000], M[100000];

void Input()
{
    cin >> n;
    for (int i = 1; i <= n; i ++){ cin >> A[i]; }
}

void Initialize(int node, int first, int last)
{
    /// Ta biểu diễn cây bằng một mảng M[1,2∗2[logN]+1]
    /// với M[i] là vị trí có giá trị nhỏ nhất trong đoạn mà nút i quản lý.
    if (first == last){
        M[node] = A[first];
        return;
    }
    Initialize(node*2, first, (first+last)/2);
    Initialize(node*2+1, (first+last)/2+1, last);
    M[node] = min(M[node*2], M[node*2+1]);

}

int main()
{
    freopen("segment_tree.inp", "r", stdin);
    freopen("segment_tree.out", "w", stdout);


    Input();
    Initialize(1, 1, n);
    for (int i = 1; i <= 4*n; i ++){
        cout << M[i] << " ";
    }
}
