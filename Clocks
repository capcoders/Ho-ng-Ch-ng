#include <iostream>
#include <stdio.h>
#define INF 100
using namespace std;
int bestWay = INF, Clock[1000], X[1000];

void readData()
{
    for (int i = 1; i <= 9; i ++){ cin >> Clock[i]; }
}

void writeData()
{
    for (int i = 1; i <= 9; i ++){ cout << X[i] << " "; }
    cout << endl;
}

void Turn(int i)
{
    Clock[i] = (Clock[i] + 1) % 4;
}

void Switch(int i)
{
    switch (i){
        case 1:
            Turn(1);
            Turn(2);
            Turn(4);
            Turn(5);
            break;
        case 2:
            Turn(1);
            Turn(2);
            Turn(3);
            break;
        case 3:
            Turn(2);
            Turn(3);
            Turn(5);
            Turn(6);
            break;
        case 4:
            Turn(1);
            Turn(4);
            Turn(7);
            break;
        case 5:
            Turn(2);
            Turn(4);
            Turn(5);
            Turn(6);
            Turn(8);
            break;
        case 6:
            //3 6 9
            Turn(3);
            Turn(6);
            Turn(9);
            break;
        case 7:
            Turn(4);
            Turn(5);
            Turn(7);
            Turn(8);
            break;
        case 8:
            Turn(7);
            Turn(8);
            Turn(9);
            break;
        case 9:
            Turn(5);
            Turn(6);
            Turn(8);
            Turn(9);
            break;
    }
}

void checkSwitch()
{
    for (int i = 1; i <= 9; i ++){
        if (X[i] == 1){
            Switch(i);
        }
    }
}

void checkResult()
{
    int count = 0;
    for (int i = 1; i <= 9; i ++){
        if (Clock[i] == 0) { count ++; }
    }
    if (count == 9){

    }
}

void Try(int i)
{
    for (int j = 0; j <= 1; j ++){
        X[i] = j;
        if (i == 9){
            //writeData();
            checkSwitch();
            checkResult();
        }
        }else{
            Try(i+1);
        }
    }
}

int main()
{
    freopen("clocks.inp", "r", stdin);
    freopen("clocks.out", "w", stdout);

    readData();
    Try(1);
}
